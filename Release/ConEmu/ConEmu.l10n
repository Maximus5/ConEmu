  "cmnhints": {
    "bRealConsoleSettings": {
      "en": "You may change font face and size in the Real console. Console restart required.",
      "ru": "Вы можете изменить имя шрифта и размер для RealConsole. Требуется перезапуск консоли.",
      "zh": "你可以改变真实控制台（Real console）的字体外观和大小。需要重启控制台（Console）。",
      "id": 1608 }
    ,
    "bReloadSettings": {
      "en": "Reload all settings from registry/xml",
      "zh": "从注册表或xml文件重新加载所有配置",
      "id": 1596 }
    ,
    "bResetSettings": {
      "en": "Reset all settings to defaults",
      "zh": "重置所有配置为默认配置",
      "id": 1595 }
    ,
    "bSaveSettings": {
      "en": [ "Save settings to registry/xml\r\n"
            , "Don't close dialog if Shift pressed" ],
      "zh": [ "保存设置到注册表或xml文件\r\n"
            , "如果按下Shift键不会关闭对话框" ],
      "id": 1610 }
    ,
    "cbActivateSplitMouseOver": {
      "en": [ "When several panes (splits) are visible simultaneously activate (put focus in) console with mouse hover,\r\n"
            , "3rd-state - match ‘Active window tracking’ system settings" ],
      "zh": [ "当一些窗格（分割窗口）可见并同时激活时（聚焦在） 鼠标悬停的控制台上，\r\n"
            , "第三种情况 - 匹配 ‘激活的窗口跟踪’ 系统设置" ],
      "id": 2601 }
    ,
    "cbAdminShield": {
      "en": "When this is checked - ‘Shield’ icon will be shown in tabs, started ‘As administrator’",
      "zh": "如果选中此处 - “盾牌” 图标会显示在Tab页上，以管理员权限“As administrator”启动",
      "id": 2109 }
    ,
    "cbAdminSuffix": {
      "en": [ "When this is checked - specified suffix will be appended in tabs titles, started ‘As administrator’.\r\n"
            , "You may choose insertion place with ‘%a’ var, otherwise suffix will be at the tab end." ],
     "zh": [ "如果选中此处 - 指定的后缀会被追加在Tab页的标题上，以管理员权限“As administrator”启动.\r\n"
            , "你可以使用变量‘%a’选择插入位置，否则后缀会插入在Tab最后面" ],
      "id": 2110 }
    ,
    "cbAlwaysOnTop": {
      "en": "Places the ConEmu window above all non-topmost windows. The window maintains its topmost position even when it is deactivated",
      "zh": "把ConEmu窗口放置在其他非置顶窗口的前面。即使在不激活的状态下，窗口会一直置顶",
      "id": 1572 }
    ,
    "cbAlwaysShowTrayIcon": {
      "en": "Always show ConEmu icon in the Taskbar Status Area (TSA)",
      "zh": "总是显示ConEmu图标在任务栏状态区域(TSA)",
      "id": 1736 }
    ,
    "cbAnsiLog": {
      "en": [ "Log console output to text files\r\n"
            , "ConEmu-YYYY-MM-DD-pNNNN.log\r\n"
            , "(affect new consoles only)" ],
      "ru": [ "Логировать консольный вывод в текстовые файлы\r\n"
            , "ConEmu-YYYY-MM-DD-pNNNN.log\r\n"
            , "(влияет только на новые консоли)" ],
      "zh": [ "输出控制台（console）日志到文本文件\r\n"
            , "ConEmu-YYYY-MM-DD-pNNNN.log\r\n"
            , "（仅影响之后新建的控制台（console））" ],
      "id": 2679 }
    ,
    "cbAutoRegFonts": {
      "en": "Search, register for process, and use first *.ttf file in conemu folder",
      "zh": "查找、注册进程，并且使用conemu文件夹中的第一个*.ttf文件",
      "id": 1550 }
    ,
    "cbBgAllowPlugin": {
      "en": [ "Enable background Far plugins (e.g. Panel Colorer),\r\n"
            , "3rd-state - don't draw in viewer/editor" ],
      "zh": [ "启用背景Far插件（比如 窗格颜色美化），\r\n"
            , "第三种情况 - 不要在视图/编辑器（viewer/editor）中绘制" ],
      "id": 1734 }
    ,
    "cbBgImage": {
      "en": [ "Show background image instead standard\r\n"
            , "colors #0 and #1 (black and blue)\r\n"
            , "3rd-state - don't draw in viewer/editor" ],
      "zh": [ "现实背景图片取代标准背景\r\n"
            , "颜色 #0 和 #1 （黑和蓝）\r\n"
            , "第三种情况 - 不要在视图/编辑器（viewer/editor）中绘制" ],
      "id": 1699 }
    ,
    "cbBold": {
      "en": "Main font weight",
      "zh": "主字体加粗",
      "id": 2278 }
    ,
    "cbClinkWebPage": {
      "en": "Open clink's web page https://mridgers.github.io/clink/ in your browser",
      "zh": "在浏览器中打开clink项目的主页 https://mridgers.github.io/clink/",
      "id": 2614 }
    ,
    "cbCloseConsoleConfirm": {
      "en": "Show confirmation before closing tab or page (tab ‘Close’ menu item)",
      "zh": "关闭标签（Tab）或页面时需要确认 (标签页“关闭”菜单项)",
      "id": 2179 }
    ,
    "cbCloseEditViewConfirm": {
      "en": "Show confirmation before closing Far Manager editor or viewer tab",
      "zh": "关闭Far Manager编辑器或视图标签（Tab）时需要确认",
      "id": 2517 }
    ,
    "cbCloseWindowConfirm": {
      "en": "Show confirmation before closing ConEmu window (cross clicking)",
      "zh": "关闭ConEmu窗口时需要确认 （cross clicking）",
      "id": 2757 }
    ,
    "cbComspecTest": {
      "en": "Show ‘calculated’ ComSpec path",
      "zh": "显示ComSpec路径的“计算值”",
      "id": 2162 }
    ,
    "cbComspecUpdateEnv": {
      "en": "ConEmu may update %ComSpec% variable to selected command processor",
      "zh": "ConEmu更新%ComSpec%变量至选定的命令处理器",
      "id": 2160 }
    ,
    "cbConfirmCloseRunning": {
      "en": "If there are two or more user processes in the console",
      "zh": "如果在控制台（Console）里有两个或更多的用户进程",
      "id": 2758 }
    ,
    "cbConsoleExceptionHandler": {
      "en": [ "By default ConEmu doesn't do SetUnhandledExceptionFilter in alternative servers (Far, Telnet, etc.)\r\n"
            , "But it may be enabled for debugging purposes" ],
      "zh": [ "默认情况下ConEmu不会在可替代的服务里（Far, Telnet, 等等）使用未处理异常过滤器（SetUnhandledExceptionFilter）\r\n"
            , "但可以在调试情形下启用" ],
      "id": 2622 }
    ,
    "cbCTSAutoCopy": {
      "en": "Copy immediately on left mouse button up (when selecting with mouse)",
      "zh": "鼠标左键释放时立即复制（用鼠标选中文本时）",
      "id": 2340 }
    ,
    "cbCTSBashMargin": {
      "en": "Bash leave one space-character on the right edge of console when wrapping lines, skip these spaces when copying to clipboard",
      "zh": "在封装各行时，Bash会在控制台（Console）最右边留一个空格字符，粘贴到剪贴板时会跳过此空格",
      "id": 2359 }
    ,
    "cbCTSBlockSelection": {
      "en": "Enable rectangular selection (like standard Windows console)",
      "zh": "启用矩形选择（和标准的Windows控制台console一样)",
      "id": 1635 }
    ,
    "cbCTSClickPromptPosition": {
      "en": "3rd-state means ‘try to detect if console is in ReadConsole’, if you want to use this with bash - turn checkbox to ‘On’ (don't use third state)",
      "zh": "第三种情况 意思是“如果控制台console在ReadConsole里，尝试自动探测console”, 如果你想在使用bash时使用此功能 - 把复选框选为 ‘On’ （不要使用第三种情况）",
      "id": 2344 }
    ,
    "cbCTSDeleteLeftWord": {
      "en": [ "Delete word leftward to the cursor (ignored in Far),\r\n"
            , "3rd-state means ‘try to detect if console is in ReadConsole’, if you want to use this with bash - turn checkbox to ‘On’ (don't use third state)" ],
      "zh": [ "删除光标左边的单词（在Far里会忽略）,\r\n"
            , "第三种情况 意思是“如果控制台console在ReadConsole里，尝试自动探测console”, 如果你想在使用bash时使用此功能 - 把复选框选为 ‘On’ （不要使用第三种情况）" ],
      "id": 2526 }
    ,
    "cbCTSDetectLineEnd": {
      "en": "Try to copy wrapped text as single line when possible (Text selection)",
      "zh": "如果可能，尝试复制封装为单行的文本（文本选择）",
      "id": 2358 }
    ,
    "cbCTSEndCopyBefore": {
      "en": "Copy selection to clipboard before selection region reset",
      "zh": "在选择区域重置前复制所选内容到剪贴板",
      "id": 2579 }
    ,
    "cbCTSEndOnKeyPress": {
      "en": [ "When ‘Off’ - only characters ends selection\r\n"
            , "When ‘On’ - any key ends selection (Arrows, PgUp, End, etc.)" ],
      "zh": [ "当为‘Off’时 - 仅字符键结束选择\r\n"
            , "当为‘On’时 - 任意键结束选择（Arrows, PgUp, End,等）" ],
      "id": 2436 }
    ,
    "cbCTSEndOnTyping": {
      "en": "Reset selection region on keypress",
      "zh": "按下键盘按键时重置选择区域",
      "id": 2412 }
    ,
    "cbCTSFreezeBeforeSelect": {
      "en": "Freeze console contents before selection (may cause small lag)",
      "zh": "在选择前冻结控制台内容（可能导致轻微滞后）",
      "id": 2257 }
    ,
    "cbCTSIBeam": {
      "en": "Show IBeam (text selection) cursor when available and allowed by settings and key modifiers",
      "zh": "在可用并被程序配置和键修饰符（key modifiers）设置为允许的情况下，显示IBeam（文本选择）光标",
      "id": 2603 }
    ,
    "cbCTSShiftArrowStartSel": {
      "en": "Start text selection with Shift+Left/Right/Home/End or block selection with Shift+Up/Down",
      "zh": "以Shift+Left/Right/Home/End键开始文本选择，或者，按下Shift+Up/Down键开始块状模式选择",
      "id": 2553 }
    ,
    "cbCTSTextSelection": {
      "en": "Enable streaming selection (like GUI text editors)",
      "zh": "启用流选择（类似图形界面文本选择）",
      "id": 1630 }
    ,
    "cbCTSTrimTrailing": {
      "en": "3rd-state means ‘Only for Text selection’",
      "zh": "第三种情况 意思是“仅文本选择”",
      "id": 2365 }
    ,
    "cbCursorBlink": {
      "en": [ "When \"Blinking\" is ON - cursor blinks\r\n"
            , "with standard cursor blink rate." ],
      "zh": [ "当 \"闪烁\" 选项开启 - 光标会闪烁\r\n"
            , "闪烁在标准频率下" ],
      "id": 1522 }
    ,
    "cbCursorColor": {
      "en": [ "ON - cursor emulates console behaviour (sort of colors inversion)\r\n"
            , "OFF - cursor is white (color#15) on dark backgrounds,\r\n"
            , "and black (color#0) on light backgrounds; also this checkbox affect appearance of ‘Block’ cursors" ],
      "zh": [ "ON - 光标模拟控制台行为（各种颜色转化）\r\n"
            , "OFF - 光标为白色 (颜色#15) 背景灰暗\r\n"
            , "以及光标黑色(颜色#0) 背景明亮；这个复选框同时也会影响块状（“Block”）光标的外观" ],
      "id": 1210 }
    ,
    "cbCursorIgnoreSize": {
      "en": "Ignore cursor size in real console, always show cursor with specified size",
      "zh": "忽略在真实控制台（real console）中的光标大小，总是以指定大小显示光标",
      "id": 1655 }
    ,
    "cbDebugSteps": {
      "en": [ "Show some debug information in ConEmu title\r\n"
            , "E.g. plugin communication steps." ],
      "zh": [ "在ConEmu标题里显示一些调试信息\r\n"
            , "比如：插件交互步骤" ],
      "id": 1551 }
    ,
    "cbDefTermAgressive": {
      "en": "If enabled, running ConEmu instance will not be required for hooking new processes. One already hooked process, e.g. ‘Explorer.exe’, will be enough.",
      "zh": "如果被启用，运行中的ConEmu实例不会被请求hook新的进程。已经被hook的进程已经足够，例如 ‘Explorer.exe’。",
      "id": 2684 }
    ,
    "cbDisableAllFlashing": {
      "en": [ "Disable all flashing (from consoles) on Windows taskbar.\r\n"
            , "3rd-state - allow short flashing." ],
      "zh": [ "停止Windows任务栏上所有来自控制台（Console）的闪烁。\r\n"
            , "第三种情况 - 允许较短闪烁" ],
      "id": 1719 }
    ,
    "cbDisableFarFlashing": {
      "en": [ "Disable flashing (from Far Manager) on Windows taskbar.\r\n"
            , "3rd-state - allow short flashing." ],
      "zh": [ "停止Windows任务栏上所有来自Far Manager的闪烁。\r\n"
            , "第三种情况 - 允许较短闪烁" ],
      "id": 1718 }
    ,
    "cbDnDCopy": {
      "en": [ "Forced \"Copy\" action as default,\r\n"
            , "You can use Ctrl, Shift or Alt\r\n"
            , "during drag to change action" ],
      "zh": [ "强制复制行为\"Copy\"为默认行为，\r\n"
            , "你能使用Ctrl, Shift 或者 Alt\r\n"
            , "在拖放时改变行为" ],
      "id": 2287 }
    ,
    "cbDosBox": {
      "en": "Enables DosBox integration",
      "zh": "启用DosBox集成",
      "id": 1753 }
    ,
    "cbDragIcons": {
      "en": "Show icons of dragged items",
      "zh": "显示拖放项目的图标",
      "id": 1553 }
    ,
    "cbDragImage": {
      "en": "Create and display overlay transparent snapshot of files are dragged",
      "zh": "创建并显示被拖放文件的覆盖透明快照",
      "id": 1548 }
    ,
    "cbDragL": {
      "en": "Allow drag with left mouse button",
      "zh": "允许使用鼠标左键拖放",
      "id": 1504 }
    ,
    "cbDragPanel": {
      "en": [ "Enable sizing of left and right panel by dragging with mouse.\r\n"
            , "3rd-state - resize on button release with macroses." ],
      "zh": [ "通过拖放鼠标启用左右窗格\r\n"
            , "第三种情况 - 按钮释放时通过宏指令调整大小" ],
      "id": 1568 }
    ,
    "cbDragR": {
      "en": "Allow drag with right mouse button",
      "zh": "允许用鼠标右键拖放",
      "id": 1505 }
    ,
    "cbDropEnabled": {
      "en": [ "Off - ConEmu will NOT receive external drops\r\n"
            , "On - Drops allowed (Far confirmation dialog)\r\n"
            , "3rd-state - same as \"On\", but w/o confirmation" ],
      "zh": [ "Off - ConEmu不会接受外部拖放\r\n"
            , "On - 允许拖放 (Far 确认对话框)\r\n"
            , "第三种情况 - 同\"On\"，但无 w/o 确认" ],
      "id": 1502 }
    ,
    "cbDropUnlocked": {
      "en": "Unlock source and target window on Drop operation. This may cause unpredictable results, when DragSource creates temp dragged files ONLY for drag lifetime.",
      "zh": "通过拖放解锁源窗口和目标窗口。当拖放源仅在拖放进行时创建临时文件，可能会导致不预期的结果。",
      "id": 1579 }
    ,
    "cbDropUseBCopy": {
      "en": "Use BCopy service for drop operations (‘Unlocked drop’ must be checked)",
      "zh": "拖放操作时使用BCopy服务（“解锁拖放（Unlocked drop）”必须被选中）",
      "id": 1580 }
    ,
    "cbEnableMouse": {
      "en": "Uncheck this to stop sending mouse events to console, mouse will be processed by ConEmu internally (text selection, for example)",
      "zh": "不选中时停止发送鼠标事件到控制台console，鼠标事件会在ConEmu内部处理（例如，文本选择）",
      "id": 2313 }
    ,
    "cbEnhanceGraphics": {
      "en": "Enhance appearance of progressbars and scrollbars",
      "zh": "增强进度栏和滚动条的外观",
      "id": 2289 }
    ,
    "cbExportConfig": {
      "en": [ "Export your current settings to xml file\r\n"
            , "\r\n"
            , "Beware overwriting your current ConEmu.xml if you are using ConEmu portable mode!" ],
      "zh": [ "导出当前配置到xml文件\r\n"
            , "\r\n"
            , "如果你正在使用的是便携模式，注意会覆盖你当前的ConEmu.xml文件！" ],
      "id": 2572 }
    ,
    "cbExtendColors": {
      "en": [ "You can use up to 32 foreground colors\r\n"
            , "(instead of console standard 16) at the\r\n"
            , "expense of one of background colors" ],
      "zh": [ "你可以使用32多种前端颜色\r\n"
            , "（取代控制台console标准的16种）\r\n"
            , "背景颜色会占用一种颜色" ],
      "id": 1515 }
    ,
    "cbExtendFonts": {
      "en": [ "ConEmu is able to use normal, bold and italic fonts side by side.\r\n"
            , "This feature may be useful for prompt decoration or with Colorer plugin for Far Manager (1.7x, 2.x)." ],
      "zh": [ "ConEmu可以同时使用normal、bold和italic字体。\r\n"
            , "这个特征可能对于提示性装饰很有用或者和Far Manager的颜色插件配合使用 (1.7x, 2.x)." ],
      "id": 1576 }
    ,
    "cbExtendUCharMap": {
      "en": "Show glyphs from selected font in ‘Unicode CharMap’ plugin (FAR2 only)",
      "zh": "显示从‘Unicode CharMap’插件里选择的字体的象形文字（仅支持FAR2）",
      "id": 1722 }
    ,
    "cbFadeInactive": {
      "en": "When ConEmu loses focus, its contents may be faded. You may specify most bright color.",
      "zh": "当不再聚焦在ConEmu上时，它的内容可能会褪色。你可以指定比较明亮的颜色。",
      "id": 1581 }
    ,
    "cbFarHourglass": {
      "en": "Show AppStarting cursor (arrow with a small hourglass) when Far is not responding (during long operations)",
      "zh": "在需要较长时间操作的过程中，如果Far不响应，显示程序启动光标（带一个小沙漏的箭头）",
      "id": 1563 }
    ,
    "cbFARuseASCIIsort": {
      "en": [ "Hook Far string sort functions.\r\n"
            , "!!!HIGHLY EXPERIMENTAL!!!" ],
      "zh": [ "Hook Far字符串排序函数。\r\n"
            , "!!!高度实验性功能!!!" ],
      "id": 1561 }
    ,
    "cbFixAltOnAltTab": {
      "en": "When You set a macro on Alt (RAlt) it can unexpectedly activates on AltTab or AltF9. This issue can be fixed by sending to console Control depress before Alt release.",
      "zh": "当你在Alt (右Alt)键设置一个宏时，可能会不按照预期的激活AltTab或AltF9。这个问题可以在Alt释放前，通过向控制台发送按下Conctrol键的消息而解决",
      "id": 1562 }
    ,
    "cbFixFarBorders": {
      "en": [ "You can specify additional font for drawing pseudographics or specified range of symbols.\r\n"
            , "E.g. main - \"Fixedsys\", additional - \"Lucida Console\".\r\n"
            , "You may force this font for specified range of symbols, search \"Settings-ConEmu.reg\" for \"FixFarBordersRanges\" option." ],
      "zh": [ "你可以为错误代码页或指定范围内的符号指定额外的字体。\r\n"
            , "例如：主要 - \"Fixedsys\", 额外 - \"Lucida Console\".\r\n"
            , "你可以为指定范围的符号强制使用此字体，可在\"Settings-ConEmu.reg\" 中查找\"FixFarBordersRanges\"选项。" ],
      "id": 1207 }
    ,
    "cbFocusInChildWindows": {
      "en": "Set focus in xshell/putty/notepad/etc. when ConEmu is activated or tab switched.",
      "zh": "当ConEmu激活时或者切换标签页时，聚焦在xshell/putty/notepad/等窗口上。",
      "id": 2568 }
    ,
    "cbFontAuto": {
      "en": "Automatic font resize for the fixed real console width",
      "zh": "对于固定宽度的真实控制台（real console）自动重新设置字体大小",
      "id": 1566 }
    ,
    "cbHandleFarDetach": {
      "en": "Automatic attach to ConEmu new Far manager console, created on detach (CtrlAltTab)",
      "zh": "自动附加到ConEmu分离时创建的新的Far manager console (CtrlAltTab)",
      "id": 1619 }
    ,
    "cbHereName": {
      "en": [ "What you will see in the Explorer popup menu\r\n"
            , "You can use `&` sign to set hotkey for the menu item" ],
      "zh": [ "Explorer的弹出菜单中你所看见的内容\r\n"
            , "你可以使用 `&` 标志为菜单项设置热键" ],
      "id": 2407 }
    ,
    "cbHideCaption": {
      "en": "Hide main window caption, when maximized (Alt-F9)",
      "zh": "最大化时，隐藏主窗口标题 (Alt-F9)",
      "id": 1565 }
    ,
    "cbHideCaptionAlways": {
      "en": "Remove window border and caption. Only console, tabs and status bar will be shown.",
      "zh": "移除窗口边界和标题栏。仅显示控制台（console）、标签页（tabs）和状态栏",
      "id": 1569 }
    ,
    "cbHideDisabledTabs": {
      "en": [ "Hide tabs, which can't be activated.\r\n"
            , "E.g. hide Far Manager editors while executing cmd.exe" ],
      "zh": [ "隐藏不能被激活的标签页（tabs）。\r\n"
            , "例如：执行cmd.exe时隐藏Far Manager编辑器" ],
      "id": 2051 }
    ,
    "cbHideInactiveConTabs": {
      "en": "Show tabs from active console only",
      "zh": "仅显示活动的控制台（console）的标签页（tabs）",
      "id": 1642 }
