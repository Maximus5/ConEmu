"$(OUTFULLNAME)": $(DEF_FILE) $(LINK_OBJS) $(RES_FILE) $(MINGWRES) conemu.gcc.manifest
	@echo $(CLR_BRN)linking $@$(CLR_STD)
	@echo :: $(LINKFLAGS) $(LINK_LIBS)
	$(LNK) @<<
	$(LINKFLAGS) $(LINK_OBJS) $(RES_FILE) $(LINK_LIBS)
<<
